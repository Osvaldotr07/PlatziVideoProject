{
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": ["airbnb"],
    "globals": {
      "document": false,
      "escape": false,
      "navigator": false,
      "unescape": false,
      "window": false,
      "describe": true,
      "before": true,
      "it": true,
      "expect": true,
      "sinon": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "react/jsx-filename-extension": 0,
      "array-bracket-spacing": 4,
      "arrow-body-style": 0,
      "block-scoped-var": 4,
      "brace-style": [4, "1tbs", { "allowSingleLine": true }],
      "comma-spacing": [4, { "before": true, "after": true }],
      "comma-style": [4, "last"],
      "complexity": 0,
      "consistent-return": 1,
      "consistent-this": 0,
      "curly": [4, "multi-line"],
      "default-case": 0,
      "dot-location": [4, "property"],
      "dot-notation": 0,
      "eol-last": 4,
      "eqeqeq": [4, "allow-null"],
      "func-names": 0,
      "func-style": 0,
      "generator-star-spacing": [4, "both"],
      "guard-for-in": 0,
      "handle-callback-err": [4, "^(err|error|anySpecificError)$" ],
      "indent": [4, 4, { "SwitchCase": 1 }],
      "key-spacing": [4, { "beforeColon": false, "afterColon": true }],
      "linebreak-style": 0,
      "max-depth": 0,
      "max-len": [4, 1550, 4],
      "max-nested-callbacks": 0,
      "max-params": 0,
      "max-statements": 0,
      "new-cap": [4, { "newIsCap": true, "capIsNew": false }],
      "newline-after-var": [0, "never"],
      "new-parens": 4,
      "no-alert": 0,
      "no-array-constructor": 4,
      "no-bitwise": 0,
      "no-caller": 4,
      "no-catch-shadow": 0,
      "no-cond-assign": 4,
      "no-console": 0,
      "no-constant-condition": 0,
      "no-continue": 0,
      "no-control-regex": 4,
      "no-debugger": 0,
      "no-delete-var": 4,
      "no-div-regex": 0,
      "no-dupe-args": 4,
      "no-dupe-keys": 4,
      "no-duplicate-case": 4,
      "no-else-return": 4,
      "no-empty": 0,
      "no-empty-character-class": 4,
      "no-labels": 4,
      "no-eq-null": 0,
      "no-eval": 4,
      "no-ex-assign": 4,
      "no-extend-native": 4,
      "no-extra-bind": 4,
      "no-extra-boolean-cast": 4,
      "no-extra-parens": 0,
      "no-extra-semi": 0,
      "no-extra-strict": 0,
      "no-fallthrough": 4,
      "no-floating-decimal": 4,
      "no-func-assign": 4,
      "no-implied-eval": 4,
      "no-inline-comments": 0,
      "no-inner-declarations": [4, "functions"],
      "no-invalid-regexp": 4,
      "no-irregular-whitespace": 4,
      "no-iterator": 4,
      "no-label-var": 4,
      "no-lone-blocks": 0,
      "no-lonely-if": 0,
      "no-loop-func": 0,
      "no-mixed-requires": 0,
      "no-mixed-spaces-and-tabs": [4, false],
      "no-multi-spaces": 4,
      "no-multi-str": 0,
      "no-multiple-empty-lines": [4, { "max": 1 }],
      "no-native-reassign": 4,
      "no-negated-in-lhs": 4,
      "no-nested-ternary": 0,
      "no-new": 4,
      "no-new-func": 4,
      "no-new-object": 4,
      "no-new-require": 4,
      "no-new-wrappers": 4,
      "no-obj-calls": 4,
      "no-octal": 4,
      "no-octal-escape": 4,
      "no-path-concat": 0,
      "no-plusplus": 0,
      "no-process-env": 0,
      "no-process-exit": 0,
      "no-proto": 4,
      "no-redeclare": 4,
      "no-regex-spaces": 4,
      "no-reserved-keys": 0,
      "no-restricted-modules": 0,
      "no-script-url": 0,
      "no-self-compare": 4,
      "no-sequences": 4,
      "no-shadow": 0,
      "no-shadow-restricted-names": 4,
      "no-spaced-func": 4,
      "no-sparse-arrays": 4,
      "no-sync": 0,
      "no-ternary": 0,
      "no-throw-literal": 4,
      "no-trailing-spaces": 4,
      "no-undef": 0,
      "no-undef-init": 4,
      "no-undefined": 0,
      "no-underscore-dangle": 0,
      "no-unneeded-ternary": 4,
      "no-unreachable": 4,
      "no-unused-expressions": 0,
      "no-unused-vars": [4, { "vars": "all", "args": "none" }],
      "no-var": 4,
      "no-void": 0,
      "no-warning-comments": 0,
      "no-with": 4,
      "object-curly-newline": 0,
      "object-curly-spacing": [4, "always"],
      "one-var": 0,
      "operator-assignment": 0,
      "operator-linebreak": [4, "after"],
      "padded-blocks": 0,
      "prefer-const": 4,
      "quote-props": 0,
      "quotes": [4, "single", "avoid-escape"],
      "radix": 4,
      "jsx-quotes": [4, "prefer-single"],
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "jsx-a11y/media-has-caption": 0,
      "react/display-name": 0,
      "react/jsx-boolean-value": 0,
      "react/jsx-closing-bracket-location": 4,
      "react/jsx-curly-spacing": [4, "never"],
      "react/jsx-equals-spacing": [4, "never"],
      "react/jsx-indent-props": [4, 4],
      "react/jsx-no-bind": [4, { "allowArrowFunctions": true }],
      "react/jsx-no-undef": 4,
      "react/jsx-pascal-case": 4,
      "react/jsx-sort-prop-types": 0,
      "react/jsx-sort-props": 0,
      "react/jsx-uses-react": 4,
      "react/jsx-uses-vars": 4,
      "react/no-did-mount-set-state": 4,
      "react/no-did-update-set-state": 4,
      "react/no-multi-comp": 0,
      "react/no-unknown-property": 4,
      "react/prop-types": 0,
      "react/forbid-prop-types": 0,
      "react/prefer-stateless-function": 0,
      "react/require-default-props": 0,
      "react/react-in-jsx-scope": 4,
      "react/self-closing-comp": 4,
      "react/sort-comp": 0,
      "react/jsx-wrap-multilines": 4,
      "semi-spacing": 0,
      "sort-vars": 0,
      "space-before-blocks": [4, "always"],
      "space-before-function-paren": [4, {"anonymous": "always", "named": "never"}],
      "space-in-parens": [4, "never"],
      "space-infix-ops": 4,
      "keyword-spacing": 4,
      "space-unary-ops": [4, { "words": true, "nonwords": false }],
      "spaced-comment": [0, "always"],
      "strict": 0,
      "use-isnan": 4,
      "valid-jsdoc": 0,
      "valid-typeof": 4,
      "vars-on-top": 4,
      "wrap-iife": [4, "any"],
      "wrap-regex": 0,
      "yoda": [4, "never"]
    }
  }